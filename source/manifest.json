{
	"name": "AWS Quick Delete",
	"version": "0.0.1",
	"description": "AWS Console Quick Delete - Delete AWS resources without typing confirmation.",
	"homepage_url": "https://dohad.dev",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "https://*.aws.amazon.com/*"
      ],
      "js": [
        "enable-button.js"
      ]
    }
  ]
}
